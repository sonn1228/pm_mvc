mixin search(objSearch)
  form(form-search)
    .input-group
      input(
        type="text"
        placeholder="Nhập từ khóa"
        name="keyword"
        class="form-control"
        value=objSearch.keyword
      )
      .input-group-append
        button(
          class="btn btn-success"
          type="submit"
        ) Tìm
